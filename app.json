{
  "expo": {
    "name": "The Flash Cards",
    "slug": "The Flash Cards",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/icone_theflashcards.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icone_theflashcards.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.davidlsousa.theflashcards",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone_theflashcards.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icone_theflashcards.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
