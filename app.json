{
  "expo": {
    "name": "The Flash Cards",
    "slug": "theflashcards",
    "version": "1.1.0",
    "orientation": "default",
    "icon": "./assets/images/icone_theflashcards.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/icone_theflashcards.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.davidlsousa.theflashcards",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icone_theflashcards.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icone_theflashcards.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "09349fc3-b895-4158-a666-9d686aa31a1c"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/09349fc3-b895-4158-a666-9d686aa31a1c"
    }
  }
}
